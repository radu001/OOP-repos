<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

	<servlet>
        <servlet-name>AddServlet</servlet-name>
        <servlet-class>com.google.AddServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>GetMarkersServlet</servlet-name>
        <servlet-class>com.google.GetMarkersServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ExploreServlet</servlet-name>
        <servlet-class>com.google.ExploreServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>SingleServlet</servlet-name>
        <servlet-class>com.google.SingleServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>UpdateServlet</servlet-name>
        <servlet-class>com.google.UpdateServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.google.LoginServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>LogOutServlet</servlet-name>
        <servlet-class>com.google.LogOutServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AddServlet</servlet-name>
        <url-pattern>/add.jsp</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>GetMarkersServlet</servlet-name>
        <url-pattern>/GetMarkersServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ExploreServlet</servlet-name>
        <url-pattern>/explore.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SingleServlet</servlet-name>
        <url-pattern>/single.jsp</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>UpdateServlet</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>LogOutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
</web-app>
